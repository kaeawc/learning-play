GET     /                           controllers.Application.index

GET     /user/:id                   controllers.Users.getById(id:Long)
GET     /user/with/email/:email     controllers.Users.findByEmail(email:String)
GET     /user/recent                controllers.Users.findRecent
POST    /user                       controllers.Users.create

GET     /assets/*file               controllers.Assets.at(path="/public", file)
